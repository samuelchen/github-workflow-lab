name: Schedule Call

on:
    schedule:
        -   cron: '*/15 * * * *'

jobs:
    eval:
        uses: samuelchen/test/.github/workflows/test.yml@master
        with:
            eval_type: retrieval
            dataset: simple

    report:
        runs-on: ubuntu-latest
        needs: eval
        steps:
            -   run: echo "${{ needs.eval.outputs.report }}"

